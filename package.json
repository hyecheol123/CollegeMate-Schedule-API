{
  "name": "collegemate-schedule-api",
  "version": "1.0.0",
  "description": "Schedule API for Collegemate project",
  "author": "Hyecheol (Jerry) Jang <hyecheol123@gmail.com>",
  "main": "dist/main.js",
  "license": "ISC",
  "keywords": [],
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testTimeout": 120000,
    "setupFiles": [
      "./test/jest.mock.ts"
    ]
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "gts clean",
    "lint": "gts lint",
    "lint:fix": "gts fix",
    "pretest": "npm run build",
    "test": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 jest --coverage --detectOpenHandles --forceExit --errorOnDeprecated --runInBand",
    "posttest": "npm run lint",
    "test:local": "cross-env COSMOS_EMULATOR_ENDPOINT=https://localhost:8081 npm test",
    "prestart": "npm run build",
    "start": "node dist/main.js"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "gts": "^3.1.1",
    "jest": "^29.5.0",
    "mockdate": "^3.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@azure/cosmos": "^3.17.3",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0"
  }
}